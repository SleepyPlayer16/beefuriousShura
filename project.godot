config_version=5

[application]

config/name="Beefurious, Shura!"
run/main_scene="res://scenes/beginGame.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="BeeFuriousShura"
config/features=PackedStringArray("4.2", "Forward Plus")
run/max_fps=60
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="res://spr_cover.png"
boot_splash/use_filter=false
config/icon="res://icon.svg"
config/windows_native_icon="res://beefuriousShuraIcon.ico"

[autoload]

Conductor="*res://scenes/Conductor.tscn"
CutsceneHandler="*res://scripts/cutsceneHandler.gd"
GameDataManager="*res://scripts/gameDataManager.gd"
SaveDataManager="*res://scripts/saveData_manager.gd"
DiscordRPCLoader="*res://addons/discord-rpc-gd/nodes/discord_autoload.gd"
DiscordRich="*res://scripts/DiscordRich.gd"
FinalBossManager="*res://scripts/finalBossManager.gd"

[debug]

gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/stretch/mode="viewport"
window/dpi/allow_hidpi=false

[editor_plugins]

enabled=PackedStringArray("res://addons/discord-rpc-gd/plugin.cfg", "res://addons/smoothing/plugin.cfg")

